<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="select">
        SELECT actor_id,first_name,last_name
        FROM actor
        WHERE actor_id = ?
    </entry>
    <entry key="select2">
        SELECT actor_id,first_name,last_name
        FROM actor
        WHERE last_name LIKE CONCAT(?,'%')
    </entry>
    <entry key="insert">
        INSERT INTO actor(first_name,last_name)
        VALUES (?,?)

    </entry>
    <entry key="update">
        UPDATE actor
        SET first_name = ?,
            last_name = ?
        WHERE actor_id = ?
    </entry>
    <entry key="delete">
        DELETE FROM actor
        WHERE actor_id = ?
    </entry>
</properties>

